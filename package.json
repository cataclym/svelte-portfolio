		"eslint-plugin-svelte": "^2.33.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rollup": "^3.15.0",
		"rollup-plugin-css-only": "^4.3.0",
		"rollup-plugin-livereload": "^2.0.0",
